{
    "manifest_version": 2,
    "name": "NYT Paywall Disabler",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "activeTab"
    ],
    "optional_permissions": [
        "topSites",
        "https://nytimes.com/*"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}